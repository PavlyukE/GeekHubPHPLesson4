{% include 'header.html.twig' %}

<div class="container">
    <form method="POST">
        <div class="row">
            <label for="first-name">Name:</label>
            <input class="form-control " type="text" name="name" value="{{ university.name }}">
        </div>
        <div class="row">
            <label for="last-name">City:</label>
            <input class="form-control" type="text" name="city" value="{{ university.city }}">
        </div>
        <div class="row">
            <label for="email">Site:</label>
            <input class="form-control" type="text" name="site" value="{{ university.site }}">
        </div>
        <div class="row">
            <input class="btn btn-default" type="submit" value="Add">
        </div>
    </form>
</div>

</body>
</html>